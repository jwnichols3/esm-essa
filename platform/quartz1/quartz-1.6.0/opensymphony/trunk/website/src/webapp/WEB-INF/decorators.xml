      <pattern>/webwork/junit/*</pattern>
      <pattern>/webwork_old/junit/*</pattern>
      <pattern>/xwork/junit/*</pattern>
      <pattern>/oscontent/junit/*</pattern>
      <pattern>/transformtags/junit/*</pattern>
      <pattern>/webcompass/junit/*</pattern>
      <pattern>/formtags/junit/*</pattern>
      <pattern>/oscore/junit/*</pattern>
      <pattern>/osuser/junit/*</pattern>
      <pattern>/osworkflow/junit/*</pattern>
      <pattern>/clickstream/junit/*</pattern>

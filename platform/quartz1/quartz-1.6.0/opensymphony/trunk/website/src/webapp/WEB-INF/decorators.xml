      <pattern>/webwork/clover/*</pattern>
      <pattern>/webwork_old/clover/*</pattern>
      <pattern>/xwork/clover/*</pattern>
      <pattern>/oscontent/clover/*</pattern>
      <pattern>/transformtags/clover/*</pattern>
      <pattern>/webcompass/clover/*</pattern>
      <pattern>/formtags/clover/*</pattern>
      <pattern>/oscore/clover/*</pattern>
      <pattern>/osuser/clover/*</pattern>

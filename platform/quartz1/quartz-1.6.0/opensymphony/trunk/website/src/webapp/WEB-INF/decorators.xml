      <pattern>/oscore/api/*</pattern>
      <pattern>/osuser/api/*</pattern>
      <pattern>/osworkflow/api/*</pattern>
      <pattern>/clickstream/api/*</pattern>
      <pattern>/propertyset/api/*</pattern>
      <pattern>/quartz/api/*</pattern>

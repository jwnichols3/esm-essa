package com.opensymphony.sass.jive;

import com.jivesoftware.base.ldap.LdapUserManager;
import com.jivesoftware.base.User;
import com.jivesoftware.base.UserNotFoundException;

/**
 * @author Patrick Lightbody (plightbo at gmail dot com)
 */
public class SassUserManager extends LdapUserManager {
    public User getUser(long l) throws UserNotFoundException {
        return super.getUser(l);
    }

    public User getUser(String string) throws UserNotFoundException {
        return super.getUser(string);
    }
}

* {
	margin: 0px;
	padding: 0px;
}

img {
	border: 0px;
}

html {
	height: 100%;
}	

body {
	background-color: #eee;
	margin: 0;
	padding: 0;
	height: 100%;
}

a, a:visited, a:active {
	color: #004A95;
}

#main {
	width: 780px;
	margin: 0 auto;
	background-color: #fff;
	border-left: 1px solid #999;
	border-right: 1px solid #999;
	padding: 10px;
	height: 100%;
}

h1, h2, h3, h4, h5, h6, p, div, li {
	font-family: arial, helvetica, san-serif;
}

ul, ol {
	margin-left: 16px;
	margin-top: 8px;
	margin-bottom: 8px;
}

p, div, li {
	font-size: 9pt;
}

p {
	margin-top: 8px;
	margin-bottom: 8px;
}

h1 {
	font-size: 20pt;
	border-bottom: 1px solid #036;
	color: #036;
	margin-top: 20px;
	margin-bottom: 10px;
}

h2 {
	font-size: 18pt;
	border-bottom: 1px solid #036;
	color: #036;
	margin-top: 20px;
	margin-bottom: 10px;
}

h3 {
	color: #036;
	font-size: 16pt;
	font-weight: bold;
	margin-top: 20px;
	margin-bottom: 0px;
}

h5 {
	font-size: 10pt;
	margin-top: 20px;
	margin-bottom: 0px;
}

.code {
	border: 1px dashed #999999; 
	padding: 5px;
	width: 535px;
	background-color: #ffc;
	margin-top: 20px;
	margin-bottom: 20px;
}

#columnCenter {
	clear: all;
	float: left;
	width: 547px;
	padding-bottom: 30px;
	margin-right: 13px;
}

#columnRight {
	margin-left: 562px;
	border-width: 0px;
	width: 214px; 
	height: 100%;
}

.box {
	float: left;
}

.panelLight, .panelDark {
	padding-top: 0px;
	padding-bottom: 8px;
	padding-left: 0px;
	padding-right: 0px;
	margin: 0px;
}

#bannerAd {
	clear: all;
	text-align: center;
}

.quickLinks {
	width: 170px;
	height: 80px;
	margin: 20px;
	text-align: center;
}

.quickLinks td { 
	width: 105px;
	text-align: center;
	font-weight: bold;
	font-size: 10pt;
	vertical-align: top;
}

.boxes {
	width: 100%;
	padding: 0px; 
	margin: 0px;
	border-collapse: collapse;
}

.boxes td {
	padding: 0px; 
	margin: 0px;
}
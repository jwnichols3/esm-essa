package com.bgi.essa.shared.exception;

/**
 * Parent for all ESSA exceptions.
 *
 * @author gsc
 *
 */
public class EssaException extends Exception {

	/**
	 * ctor
	 * 
	 * @param arg complaint
	 */
	public EssaException(final String arg) {
		super(arg);
	}
	
	/**
	 * ctor
	 * 
	 * @param arg complaint
	 */
	public EssaException(Throwable arg) {
		super(arg);
	}

	/**
	 * Generated serial version identifier. 
	 */
	private static final long serialVersionUID = 8531757410081119278L;
}

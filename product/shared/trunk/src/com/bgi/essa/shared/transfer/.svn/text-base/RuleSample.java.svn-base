package com.bgi.essa.shared.transfer;

import java.io.Serializable;

/**
 * Simple container for rule and associated sample
 * 
 * @author coleguy
 */
public class RuleSample implements Serializable {

	private DtoIf _rule;
	
	public DtoIf getRule() {
		return(_rule);
	}
	
	public void setRule(DtoIf arg) {
		_rule = arg;
	}

	private DtoIf _sample;
	
	public DtoIf getSample() {
		return(_sample);
	}
	
	public void setSample(DtoIf arg) {
		_sample = arg;
	}
	
	private VpoDTO _vpo;
	
	public VpoDTO getVpo() {
		return(_vpo);
	}
	
	public void setVpo(VpoDTO arg) {
		_vpo = arg;
	}
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 5197656443927891379L;
}

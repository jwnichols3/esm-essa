--
--
drop table essa.process;
--
--
create table essa.process (
  id int not null,
  task_id varchar(48) not null,
  rule_id varchar(48) not null,
  version int not null,
  created bigint not null,
  modified bigint not null,
  option_type varchar(16) not null,
  target varchar(256) not null,
  threshold_type varchar(16) not null,
  max_time int not null,
  min_population int not null,
  max_population int not null,
  active char(1) not null,
  note varchar(2048) not null,
  primary key(id)
);
